<%= form_for @location do |f| %>
  <%= f.label :city%>
  <%= f.text_field :city%> <br>
  <%= f.label :state%>
  <%= f.text_field :state%> <br>
  <%= f.label :username %>
  <%= f.collection_select :user_id, User.all, :id, :name, {:selected => current_user.id} %>
  <%= f.label :restaurant %> 
  <%= f.collection_select :restaurant_id, Restaurant.all, :id, :name %>
  <%= f.submit%>
<% end %>
