<%= form_for @location do |f| %>
  <%= f.label :city%>
  <%= f.text_field :city%> <br>
  <%= f.label :state%>
  <%= f.text_field :state%> <br>
  <%= f.label :username %>
  <%= f.collection_select :user_id, User.all, :id, :name, {:selected => current_user.id} %><br>
  <%= f.label :restaurant %>
  => Choose from Existing Restaurant:
  <%= f.collection_select :restaurant_id, Restaurant.all, :id, :name %>
  <br> => Or Create New: ???
  <br>
  <%= f.submit%>
<% end %>
